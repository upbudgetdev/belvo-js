{
  "name": "@upbudget/belvo-js",
  "version": "1.0.0",
  "description": "React component for Belvo",
  "main":   "dist/nextjs/index.cjs.js",
  "module": "dist/nextjs/index.esm.js",
  "types":  "dist/nextjs/index.d.ts",
  "scripts": {
    "build": "tsup",
    "prepare": "pnpm run build",
    "dev": "tsup --watch",
    "clean": "rimraf ./dist",
    "publish:local": "pnpm yalc push --replace --sig"
  },
  "keywords": [
    "belvo",
    "openfinance",
    "belvo-sdk"
  ],
  "author": "Up Budget",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^19.1.2",
    "tslib": "^2.8.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "yalc": "1.0.0-pre.53"
  },
  "exports": {
    ".": {
      "require":  "./dist/nextjs/index.cjs.js",
      "import":   "./dist/nextjs/index.esm.js",
      "types":    "./dist/nextjs/index.d.ts"
    },
    "./react": {
      "require":  "./dist/react/index.cjs.js",
      "import":   "./dist/react/index.esm.js",
      "types":    "./dist/react/index.d.ts"
    }
  },
  "files": ["dist"],
  "peerDependencies": {
    "next": "^13.5.7 || ^14.2.25 || ^15.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
